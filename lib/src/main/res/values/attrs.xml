<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClockSlider">
        <attr name="cc_borderWidth" format="dimension" />
        <attr name="cc_metricTextSize" format="dimension" />
        <attr name="cc_borderColor" format="color" />
        <attr name="cc_fillColor" format="color" />
        <attr name="cc_tickTextColor" format="color" />
        <attr name="cc_startIconResource" format="reference" />
        <attr name="cc_endIconResource" format="reference" />
        <attr name="cc_is24HR" format="boolean" />
        <attr name="cc_startHour" format="float" />
        <attr name="cc_endHour" format="float" />
        <attr name="cc_metricMode" format="enum">
            <enum name="counter" value="0" />
            <enum name="clock" value="1" />
        </attr>
    </declare-styleable>
</resources>